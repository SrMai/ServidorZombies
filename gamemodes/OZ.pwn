forward DarSed();
public DarSed()
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	if(IsPlayerConnected(i))
	{
		PlayerInfo[i][pSed] += 1;
		SetProgressBarValue(Barrased[i], PlayerInfo[i][pSed]);
		if(SedOculta[i] == 0)
		{
			UpdateProgressBar(Barrased[i], i);
		}
	}
	return 1;
}

forward versed();
public versed()
{
	for(new i = 0; i < MAX_PLAYERS; i++)
	if(IsPlayerConnected(i))
	{
		if(PlayerInfo[i][pSed] == 100)
		{
			new Float:vida;
			GetPlayerHealth(i,vida);
			SetPlayerHealth(i,vida-1);
		}
	}
	return 1;
}

