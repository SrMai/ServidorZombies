forward Reinicio();
public Reinicio()
{
	new string[128];
	if(TiempoReinicio == 1)
	{

		format(string, 128, "El servidor se reiniciará automáticamente en 1 minuto.", TiempoReinicio);
		SendClientMessageToAll(0xB1F6F6F6, string);
		SetTimerEx("Conteo", 1000, false, "i", 59);
		TiempoReinicio--;
		return 1;
	}
	if(TiempoReinicio > 0)
	{

		format(string, 128, "El servidor se reiniciará automáticamente en %d minutos.", TiempoReinicio);
		SendClientMessageToAll(0xB1F6F6F6, string);
		TiempoReinicio--;
		return 1;
	}
	if(TiempoReinicio == 0)
	{

		for(new i=0;i<MAX_PLAYERS;i++)
		{

			SaveAccount(i);
			NoQuitar[i] = 1;
			Kick(i);
		}
		SendClientMessageToAll(0xB1F6F6F6, "El servidor se reiniciara ahora.");
		KillTimer(TimerReinicio);
		SendRconCommand("exit");
	}
	return 1;
}

