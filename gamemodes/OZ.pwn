CMD:kick(playerid, params[])
{
	new string[256];
	new ID,ADMIN[256];
	if(PlayerInfo[playerid][jAdminNvl] < 3) return 0;
	if(sscanf(params, "ds[256]", ID,ADMIN) )return SendClientMessage(playerid,COLOR_WHITE,"Uso: /kick [id] [razón]");
	if(!IsPlayerConnected(ID)) return SendClientMessage(playerid,COLOR_WHITE,"Jugador desconectado o id incorrecta");
	format(string, sizeof(string), "El jugador {FF3300}%s(ID: %d){FFFFFF} fue expulsado por un admin. Razón: {FF3300}%s{FFFFFF}.",NombreJugador(ID),ID, ADMIN);
	SendClientMessageToAll(-1, string);
	Kick(ID);
	return 1;
}

CMD:unban(playerid, params[])
{
	new query[200],pName[32];
	if(PlayerInfo[playerid][jAdminNvl] < 5) return 0;
	if(sscanf(params, "s[32]", pName) )return SendClientMessage(playerid,COLOR_WHITE,"Uso: /unban [Nombre_Apellido]");
	format(query, 200, "UPDATE `usuarios` SET `Baneado`='0' WHERE `Username`='%s'", pName);
	mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
	SCM(playerid, -1, "El usuario ha sido desbaneado.");
	return 1;
}

CMD:ban(playerid, params[])
{
	new string[256],pName1[24],day,month,year;
	getdate(day,month,year);
	new ID,ADMIN[256],query[200],pName[24];
	if(PlayerInfo[playerid][jAdminNvl] < 4) return 0;
	if(sscanf(params, "rs[256]", ID,ADMIN) )return SendClientMessage(playerid,COLOR_WHITE,"Uso: /ban [id] [razón]");
	if(ID == INVALID_PLAYER_ID) return SendClientMessage(playerid,COLOR_WHITE,"Jugador desconectado o id incorrecta");
	{
		GetPlayerName(ID, pName, 24);
		GetPlayerName(playerid, pName1, 24);
		if(PlayerInfo[ID][jAdminNvl] > PlayerInfo[playerid][jAdminNvl])
		{
			format(string, sizeof(string), "El jugador {FF3300}%s(ID: %d){FFFFFF} fue auto-baneado por el anticheat. Razón: {FF3300}Intento de baneo a un ADM{FFFFFF}.",pName1, playerid);
			SendClientMessageToAll(-1, string);
			return 1;
		}
		format(string, sizeof(string), "El jugador {FF3300}%s(ID: %d){FFFFFF} fue baneado por un admin. Razón: {FF3300}%s{FFFFFF}.",NombreJugador(ID),ID, ADMIN);
		SendClientMessageToAll(-1, string);
		for(new i=0;i<MAX_PLAYERS;i++)
		{
			if(PlayerInfo[i][jAdminNvl] > 2)
			{
				format(string, 128, "Administrador: %s",pName1);
				SCM(i,COLOR_GREY,string);
			}
		}
		format(query, sizeof(query), "INSERT INTO `log_baneos` (Administrador, Baneado, Fecha,Motivo) VALUES ('%s','%s','%d/%d/%d','%s')",pName1,pName,day,month,year,ADMIN);
		mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
		format(query, 200, "UPDATE `usuarios` SET `Baneado`='1',`razon`='%s' WHERE `Username`='%s'", ADMIN,pName);
		mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
		BanEx(ID, "Admin BAN");
		return 1;
	}
}

CMD:banoff(playerid,params[])
{
	new ID[32],RAZON[100],string[200],query[200],pName[32],day,month,year;
	getdate(day,month,year);
	GetPlayerName(playerid, pName, 32);
	if(PlayerInfo[playerid][jAdminNvl] < 5) return 0;
	if(!sscanf(params,"s[32]s[100]",ID,RAZON))
	{
		format(string, sizeof(string), "El jugador {FF3300}%s{FFFFFF} fue baneado por un admin. Razón: {FF3300}%s{FFFFFF}.",ID,RAZON);
		SendClientMessageToAll(-1, string);
		format(query, sizeof(query), "INSERT INTO `log_baneos` (Administrador, Baneado, Fecha,Motivo) VALUES ('%s','%s','%d/%d/%d','%s')",pName,ID,day,month,year,RAZON);
		mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
		format(query, 200, "UPDATE `usuarios` SET `Baneado`='1',`razon`='%s' WHERE `Username`='%s'", RAZON,ID);
		mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
	}
	else return SCM(playerid,-1,"Modo de uso: /banoff [Nombre_Apellido] [Razon]");
	return 1;
}

