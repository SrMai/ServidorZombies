CMD:reportar(playerid,params[])
{
	if(PlayerInfo[playerid][pTiempoJailS] > 0) return SCM(playerid,-1,"No puedes usar este comando en este momento, en su lugar usa {DBED15}/reglas");
	new id,razon[128],string[128],pName[32],pName2[32];
	GetPlayerName(playerid,pName,32);
	if(!sscanf(params,"ds[128]",id,razon))
	{
	    if(TiempoReporte[playerid] > 0)
		{
			format(string,sizeof(string),"Debes esperar {00CC00}%d segundos{FFFFFF} para volver a reportar.",TiempoReporte[playerid]);
			SendClientMessage(playerid, COLOR_WHITE,string);
			return 1;
		}
		if(reportar[playerid] == id) return SCM(playerid,-1,"Ya has reportado a este jugador");
		if(!IsPlayerConnected(id)) return SCM(playerid,-1,"Jugador invalido");

		GetPlayerName(id, pName2, 32);
		for(new i=0;i<MAX_PLAYERS;i++)
		{
			if(PlayerInfo[i][jAdminNvl] > 2)
			{
				format(string,128,"Reporte de %s [%d] hacia %s [%d], razón: %s",pName,playerid,pName2,id,razon);
				SCM(i,0xcfc755FF,string);
				format(string,128,"Puedes aceptar el reporte utilizando /ar %d o cancelarlo utilizando /rr %d",playerid,playerid);
				SCM(i,COLOR_GREY,string);
			}
		}
		SCM(playerid,-1,"{00CC00}El reporte fue enviado correctamente.");
		reportar[playerid] = id;
		TiempoReporte[playerid] = 300;
		SetTimerEx("QuitarRepote",30000,false,"d",playerid);
		return 1;
	}
	else return SCM(playerid,-1,"Por favor, para reportar usa {DBED15}/reportar ID Razón");
}


CMD:llenar(playerid,params[])
{
	new item[32],autoid,resta,string[128];
	if(!sscanf(params,"s[32]",item))
	{
		if(strcmp(item, "tanque", true)== 0)
		{
			if(!EnGasolinera(playerid)) return 1;
			if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Debes estar en un vehiculo para llenar el tanque");
			autoid = GetPlayerVehicleID(playerid);
			if(VehicleFuel[autoid] < 100)
			{
				if(DFT(autoid))
				{
					resta = 230-VehicleFuel[autoid];
					if(PlayerInfo[playerid][jDinero] < resta*10)
					{
						format(string,128,"No tienes suficiente dinero para llenar tu vehículo $%d",resta*10);
						SCM(playerid, -1,string);
						return 1;
					}
					llenado[autoid] = SetTimerEx("DarGasolina",1000,true,"d", autoid);
					llenadop[playerid] = SetTimerEx("DarGasolinap",1000,true,"d", playerid);
					format(string,128,"Se van a cargar %d litros de combustible.", resta*10);
					SCM(playerid, -1,string);
					format(string,128,"Costo total: {00CC00}$%d", resta*10);
					SCM(playerid, -1,string);
					TogglePlayerControllable(playerid, 0);
					SetPVarInt(playerid, "IsFrozen", 0);
					QuitarDinero(playerid, resta*10);
					SinGaso[autoid] = 0;
				}
				else
				{
					resta = 100-VehicleFuel[autoid];
					if(PlayerInfo[playerid][jDinero] < resta*10)
					{
						format(string,128,"No tienes suficiente dinero para llenar tu vehículo $%d",resta*10);
						SCM(playerid, -1,string);
						return 1;
					}
					llenado[autoid] = SetTimerEx("DarGasolina",1000,true,"d", autoid);
					llenadop[playerid] = SetTimerEx("DarGasolinap",1000,true,"d", playerid);
					format(string,128,"Se van a cargar %d litros de combustible.", resta*10);
					SCM(playerid, -1,string);
					format(string,128,"Costo total: {00CC00}$%d", resta*10);
					SCM(playerid, -1,string);
					TogglePlayerControllable(playerid, 0);
					SetPVarInt(playerid, "IsFrozen", 0);
					QuitarDinero(playerid, resta*10);
					SinGaso[autoid] = 0;
				}
			}
			else return SCM(playerid, -1, "El tanque de este vehiculo esta lleno");
		}
	}
	return 1;
}

