CMD:revisar(playerid,params[])
{
	new Float:Pos[3],autoid,string[256],texto[10],id;
	if(!sscanf(params,"s[10]d",texto,id))
	{


		if(strcmp(texto,"paciente",	true) == 0)
		{


			if(PlayerInfo[playerid][jJob] != 1) return SCM(playerid,-1,"Necesitas el trabajo de médico para utilizar este comando");
			if(GetPlayerSkin(playerid) != 275) return SCM(playerid,-1,"Necesitas tener la ropa de médico para utilizar este comando");
			if(PlayerInfo[id][pEnfermedad] == 0) return 1;
			if(PlayerInfo[id][pAtendido] == 1) return SCM(playerid,-1,"El paciente ya fue atendido");
			new enfermedad[32],pName[24];
			GetPlayerName(playerid,pName,24);
			GetPlayerPos(id,Pos[0],Pos[1],Pos[2]);
			switch(PlayerInfo[id][pEnfermedad])
			{


				case 1: { enfermedad = "Herida de bala"; }
				case 2: { enfermedad = "Múltiples heridas de bala"; }
				case 3: { enfermedad = "Fracturas Graves"; }
				case 4: { enfermedad = "Fracturas leves"; }
				case 5: { enfermedad = "Herida abierta de sangre"; }
				case 6: { enfermedad = "Hematomas"; }
				case 7: { enfermedad = "Hematomas y huesos fracturados"; }
				case 8: { enfermedad = "Hemorragia cerebral"; }
				case 9: { enfermedad = "Quemaduras"; }
				case 10: { enfermedad = "Daños severos"; }
			}
			if(IsPlayerInRangeOfPoint(playerid, 2.0, Pos[0],Pos[1],Pos[2]))
			{


				format(string,sizeof(string),"El paciente presenta %s",enfermedad);
				SCM(playerid,-1,string);
				format(string,sizeof(string),"%s observa al paciente y procede a revisarlo",pName);
				ProxDetector(15.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
			}
		}
	}
	else if(sscanf(params,"s[10]d",texto,id))
	{

		if(PlayerInfo[playerid][jJob] != 8 && PlayerInfo[playerid][vipjob] != 8) return SCM(playerid,-1,"Debes ser mecánico para utilizar este comando");
		{

			for(new i=0;i<MAX_PLAYERS;i++)
			{

				if(LlamadaMeca[i] == 1 && IsPlayerInAnyVehicle(i))// return SCM(playerid,-1,"El cliente no ah llamado al servicio mecánico.");
				{

					GetPlayerPos(i, Pos[0],Pos[1],Pos[2]);
					if(IsPlayerInRangeOfPoint(playerid, 7.0, Pos[0],Pos[1],Pos[2]))// return SCM(playerid,-1,"No hay ningun cliente cerca a tu posición.");
					{

						autoid = GetPlayerVehicleID(i);
						if(Capo[autoid] != 1) return SCM(playerid,-1,"El capo del vehículo debe estar abierto");
						{

							if(autoid <= AUTOS)
							{

								SCM(playerid,-1,"El vehiculo necesita {EB0000}0{FFFFFF} repuestos");
								MRepuestos[playerid] = 0;
								return 1;
							}
							else
							{

								foreach(Player,u)
								{

									if(autos[u] == autoid)
									{

										new repuestos = PlayerInfo[u][vPrecio]/5000;
										format(string,256,"Este vehículo necesita {EB0000}%d{FFFFFF} repuestos",repuestos);
										SCM(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
									if(autos2[u] == autoid)
									{

										new repuestos = PlayerInfo[u][v2Precio]/5000;
										format(string,256,"Este vehículo necesita {EB0000}%d{FFFFFF} repuestos",repuestos);
										SCM(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
									if(autos3[u] == autoid)
									{

										new repuestos = PlayerInfo[u][v3Precio]/5000;
										format(string,256,"Este vehículo necesita {EB0000}%d{FFFFFF} repuestos",repuestos);
										SCM(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
									if(autos4[u] == autoid)
									{

										new repuestos = PlayerInfo[u][v4Precio]/5000;
										format(string,256,"Este vehículo necesita {EB0000}%d{FFFFFF} repuestos",repuestos);
										SCM(playerid,-1,string);
										MRepuestos[playerid] = repuestos;
										return 1;
									}
								}
							}
						}
					}
				}
			}
		}
	}
	return 1;
}

