CMD:mute(playerid,params[])
{
	new id,tiempo,string[128],pName[32],razon[200],string2[128],Admin[32];
	if(PlayerInfo[playerid][jAdminNvl] < 2) return 0;
	if(!sscanf(params,"dds[200]",id,tiempo,razon))
	{
		GetPlayerName(playerid, Admin, 32);
		new calculo = tiempo*60*1000;
		GetPlayerName(id,pName,32);
		TiempoDuda[id] = tiempo*60;
		SetTimerEx("DudaOn",calculo,0,"d",id);
		format(string, sizeof(string), "A %s no se le permite hablar en el chat de ayuda. Razón: %s.",pName, razon);
		SendClientMessageToAll(0xAA3333FF,string);
		for(new i=0;i<MAX_PLAYERS;i++)
		{
			if(PlayerInfo[i][jAdminNvl] > 2)
			{
				format(string, 128, "Tiempo de muteo: %d minutos, Administrador: %s",tiempo,Admin);
				SCM(i,COLOR_GREY,string);
			}
		}
		format(string2,128,"No se te permite hablar en el chat de ayuda. Por %d minuto(s)",tiempo);
		SCM(id,0xDEA600FF,string2);
	}
	else return SCM(playerid,-1,"Usa /mutear [id] [tiempo (minutos)] [razón]");
	return 1;
}

CMD:nmute(playerid,params[])
