CMD:localizar(playerid, params[]){

	new string[255], item[30], ID, amigoenc=0, contador2=0, info2[1000];

	if(!sscanf(params,"s[32]",item))
	{
		if(strcmp(item,"amigo",true)==0)
		{
			//Localizar amigo
			for(new i=0; i < MAX_PLAYERS; i++)
			{
				if(i != playerid)
				{
					if(!IsPlayerNPC(i))
					{
						if(IsPlayerConnected(i))
						{
							if(strcmp(PlayerInfo[playerid][Totem], NombreJugador(i), true) == 0)
							{
								if(strcmp(PlayerInfo[i][Totem], NombreJugador(playerid)) == 0) continue;
								amigoenc = 1;
								format(info2, sizeof(info2), "%s\n{00CCFF}%s", info2, NombreJugador(i));
								TotemBuscar[playerid][contador2] = i;
								contador2++;
							}
							else if(strcmp(PlayerInfo[i][Totem], NombreJugador(playerid), true) == 0)
							{
								amigoenc = 1;
								format(info2, sizeof(info2), "%s\n{00CCFF}%s", info2, NombreJugador(i));
								TotemBuscar[playerid][contador2] = i;
								contador2++;
							}
						}
					}
				}
			}
			if(amigoenc == 0)
			{
				format(string, sizeof(string), "No hay ningun amigo conectado actualmente en tu cuenta.");
				SendClientMessage(playerid, -1, string);
				return 1;
			}
			ShowPlayerDialog(playerid, LOCALIZARAMIGO, DIALOG_STYLE_LIST, "Localizar Amigo", info2, "Localizar", "Salir");
		}
	}
	if(!sscanf(params,"d",ID))
	{
	    if(PlayerInfo[playerid][jAdminNvl] > 3)
		{
        	format(string,128,"AUTOS DE %s", NombreJugador(ID));
			SCM(playerid,COLOR_ROJO,string);
			Localizar2(ID,playerid);
		}
	}
	if(sscanf(params, "s[30]", item))
	{
		Localizar(playerid);
	}
	return 1;
}

