			{

				SendClientMessage(playerid, COLOR_WHITE, "Te pusiste el uniforme de policía");
				//SetPlayerAttachedObject(playerid,9,19142,1,0.099999,0.071999,-0.004000,2.600000,-1.300000,-2.099998,1.000000,1.000000,1.000000);
				//SetPlayerAttachedObject(playerid,2,18636,2,0.152998,0.038000,-0.002000,86.399993,91.299995,2.299995,1.000000,1.000000,1.000000);
				//PlayerInfo[playerid][pModelo] = 211;
				PlayerInfo[playerid][pDuty] = 1;
				SetPlayerSkin(playerid, PlayerInfo[playerid][pModelo]);
				SetPlayerColor(playerid,COLOR_POLICIA); }
		}
	}
	else
	{

		SetPlayerColor(playerid,COLOR_WHITE);
		if(PlayerInfo[playerid][jSexo] == 2) {
			RemovePlayerAttachedObject(playerid,2);
			RemovePlayerAttachedObject(playerid,2); }
