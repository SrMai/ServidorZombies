public OnPlayerStateChange(playerid, newstate, oldstate)
{
	fVehSpeed[playerid] = 0.0;
    if(newstate == PLAYER_STATE_DRIVER || newstate == PLAYER_STATE_PASSENGER)
	{
		if(EmisoraEscuchando[GetPlayerVehicleID(playerid)] != -1)
		{
			new emisoraid;
			emisoraid = EmisoraEscuchando[GetPlayerVehicleID(playerid)];
			for(new id = 0; id < MAX_PLAYERS; id++)
			{
				if(Emisoras[id][CabinaOcupada] == emisoraid)
				{
					if(Emisoras[id][TransEmisora] == 1)
					{
						new stringa[128];
						EmisoraEscuchando[emisoraid]++;
						format(stringa, sizeof(stringa), "Emisora: %d {D9ED15}- %s", emisoraid, Emisoras[id][NombreEmisora]);
						SendClientMessage(playerid, COLOR_BLANCO, stringa);
						format(stringa, sizeof(stringa),"http://fenixzone.sytes.net:8000/listen.pls?sid=%d", Emisoras[id][EmisoraID]);
						PlayAudioStreamForPlayer(playerid, stringa);
						CabinasOcupadasEmisoras[emisoraid][2]++;
					}
				}
			}
		}
	}
	if(newstate == PLAYER_STATE_PASSENGER)
	{
		if(PlayerInfo[playerid][Agonizando] == 1 && PacienteAmbu[playerid] == 0)
		{
			RemovePlayerFromVehicle(playerid);
		}
		//SetPlayerArmedWeapon(playerid, 0);
		if(Objeto[playerid] > 0)
		{
			RemovePlayerFromVehicle(playerid);
		}
		if(GetPlayerWeapon(playerid) == 0 || GetPlayerWeapon(playerid) == 29 || GetPlayerWeapon(playerid) == 25 || GetPlayerWeapon(playerid) == 31 || GetPlayerWeapon(playerid) == 30) return 1;
		else
		{
			if(PlayerInfo[playerid][pWP3] == 25 || PlayerInfo[playerid][pWP4] == 29 || PlayerInfo[playerid][pWP5] == 30 ||PlayerInfo[playerid][pWP5] == 31)
			{
				DarArmas(playerid);
			}
			else
			{
				Manejando[playerid] = 1;
				ResetPlayerWeapons(playerid);
			}
		}
	}
	if(oldstate == PLAYER_STATE_PASSENGER)
	{
		if(Manejando[playerid] == 1)
		{
			DarArmas(playerid);
			Manejando[playerid] = 0;
		}
	}
	if(newstate == PLAYER_STATE_DRIVER)
	{
		if(PlayerInfo[playerid][Agonizando] == 1 && PacienteAmbu[playerid] == 0)
		{
			RemovePlayerFromVehicle(playerid);
		}
		SetPlayerArmedWeapon(playerid, 0);
		if(Objeto[playerid] > 0)
		{
			RemovePlayerFromVehicle(playerid);
		}
		new autoid = GetPlayerVehicleID(playerid);
		if(!BICI(autoid) && MotorAuto[autoid] == 0)
		{
			if(VehiculosInfo[autoid][vcomprable] != 1)
			{
				SCM(playerid,-1,"Para encender el motor del vehículo, utiliza: {DBED15}/arrancar");
			}
			SendClientMessage(playerid, COLOR_BLANCO, "Para escuchar alguna emisora de radio de San Andreas, usa {D9ED15}/emisoras");
		}
		if(VehiculosInfo[autoid][vcomprable] == 1)
		{
			new string[256];
			switch(VehiculosInfo[autoid][vmoneda])
			{
				case 0:
				{
					format(string, sizeof(string), "Este vehículo está a la venta, con un valor de {00CC00}$%d{FFFFFF}. Usa {DBED15}/comprar vehiculo{FFFFFF} para comprarlo.", VehiculosInfo[autoid][vprecio]);
					SendClientMessage(playerid, -1,string);
				}
				case 1:
				{
					format(string, sizeof(string), "Este vehículo está a la venta, con un valor de {00CC00}%d Fz{FFFFFF}. Usa {DBED15}/comprar vehiculo{FFFFFF} para comprarlo.", VehiculosInfo[autoid][vprecio]);
					SendClientMessage(playerid, -1,string);
				}
			}
		}
		new veh = GetPlayerVehicleID(playerid);
		if(JugadorLogeado[playerid] == 1)
		{
			if(BajandoV[playerid] == 1)
			{

				if(PlayerInfo[playerid][jAdminNvl] > 3) return 1;
				if(Evento[playerid] == 1) return 1;
				if(MOTO(veh) && GetPlayerState(playerid) == PLAYER_STATE_PASSENGER) return 1;
				new pName[24],string[128];
				GetPlayerName(playerid,pName,24);
				format(string, 128,"El jugador {FF3300}%s(ID: %d){FFFFFF} fue baneado por el anticheat. Razón: {FF3300}Cheat de vehículos{FFFFFF}.",pName,playerid);
				SendClientMessageToAll(-1,string);
				Kick(playerid);
			}
		}
		new Float: fCurrentSpeed, szSpeed[128];
		fCurrentSpeed = player_get_speed(playerid);
		fVehSpeed[playerid] = fCurrentSpeed;
		iVehicle[playerid] = GetPlayerVehicleID(playerid);
		if(TAXI(autoid))
		{
			EnTaxi[playerid] = 1;
			TextDrawShowForPlayer(playerid,TAXIMET[playerid]);
			TextDrawShowForPlayer(playerid,TAXIMET2[playerid]);
		}
		if(BICI(iVehicle[playerid]))
		{
			Manejando[playerid] = 1;
			//ResetPlayerWeapons(playerid);
			KillTimer(Bajando[playerid]);
			return 1;
		}
		ShowProgressBarForPlayer(playerid, speedbar2[playerid]);
		SetProgressBarValue(speedbar2[playerid],VehicleFuel[iVehicle[playerid]]);
		UpdateProgressBar(speedbar2[playerid],playerid);
		ShowProgressBarForPlayer(playerid, speedbar[playerid]);
		if(DFT(GetPlayerVehicleID(playerid)))
		{
			format(szSpeed, sizeof(szSpeed),"%d/230",VehicleFuel[iVehicle[playerid]]);
			TextDrawSetString(santiago7[playerid], szSpeed);
			format(szSpeed, sizeof(szSpeed),"%.0f", fCurrentSpeed);
			TextDrawSetString(santiago5[playerid], szSpeed);
			Manejando[playerid] = 1;
			//ResetPlayerWeapons(playerid);
			KillTimer(Bajando[playerid]);
			SetProgressBarMaxValue(speedbar2[playerid], 230);
			UpdateProgressBar(speedbar2[playerid],playerid);
		}
		else
		{
			format(szSpeed, sizeof(szSpeed),"%d/100",VehicleFuel[iVehicle[playerid]]);
			TextDrawSetString(santiago7[playerid], szSpeed);
			format(szSpeed, sizeof(szSpeed),"%.0f", fCurrentSpeed);
			TextDrawSetString(santiago5[playerid], szSpeed);
			Manejando[playerid] = 1;
			//ResetPlayerWeapons(playerid);
			KillTimer(Bajando[playerid]);
			SetProgressBarMaxValue(speedbar2[playerid], 100);
			UpdateProgressBar(speedbar2[playerid],playerid);
		}
		TextDrawShowForPlayer(playerid, santiago1[playerid] );
		TextDrawShowForPlayer(playerid, santiago2[playerid] );
		TextDrawShowForPlayer(playerid, santiago3[playerid] );
		TextDrawShowForPlayer(playerid, santiago4[playerid] );
		TextDrawShowForPlayer(playerid, santiago5[playerid] );
		TextDrawShowForPlayer(playerid, santiago6[playerid] );
		TextDrawShowForPlayer(playerid, santiago7[playerid] );
	}
	if(oldstate == PLAYER_STATE_DRIVER)
	{
		if(EnTaxi[playerid] == 1)
		{
			EnTaxi[playerid] = 0;
			TextDrawHideForPlayer(playerid,TAXIMET[playerid]);
			TextDrawHideForPlayer(playerid,TAXIMET2[playerid]);
		}
		TextDrawHideForPlayer(playerid, santiago7[playerid] );
		TextDrawHideForPlayer(playerid, santiago5[playerid] );
		TextDrawHideForPlayer(playerid, santiago1[playerid] );
		TextDrawHideForPlayer(playerid, santiago2[playerid] );
		TextDrawHideForPlayer(playerid, santiago3[playerid] );
		TextDrawHideForPlayer(playerid, santiago4[playerid] );
		TextDrawHideForPlayer(playerid, santiago6[playerid] );
		HideProgressBarForPlayer(playerid, speedbar2[playerid]);
		HideProgressBarForPlayer(playerid, speedbar[playerid]);
		//DarArmas(playerid);
		Bajando[playerid] = SetTimerEx("Maneja",5000,false,"d",playerid);
	}
	return 1;
}

