			MarihuanaInfo[i][ObjetoID] = CreateDynamicObject(19473, MarihuanaInfo[i][pX], MarihuanaInfo[i][pY], MarihuanaInfo[i][pZ]-1.78, 0.000000, 0.000000, -0.000000);
		}
		else if(MarihuanaInfo[i][Hor] == 3)
