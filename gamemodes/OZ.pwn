CMD:uniforme(playerid, params[])
{
	new texto[10];
	if(!sscanf(params,"s[10]",texto))
	{
		if(strcmp(texto,	"medico",	true) == 0)
		{
			if(PlayerInfo[playerid][jJob] == 1 || PlayerInfo[playerid][vipjob] == 1)
