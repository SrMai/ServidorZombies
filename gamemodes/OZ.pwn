		}
		format(str, sizeof(str), "{00CC00}Planta de Marihuana{FFFFFF}\nSembrada por {009BE4}%s{FFFFFF}\nMadura en %dh %dm %ds", MarihuanaInfo[i][Plantador], MarihuanaInfo[i][Hor], MarihuanaInfo[i][Min], MarihuanaInfo[i][Seg]);
		MarihuanaInfo[i][Label] = CreateDynamic3DTextLabel(str, COLOR_BLANCO, MarihuanaInfo[i][pX], MarihuanaInfo[i][pY], MarihuanaInfo[i][pZ]+0.9, 8.0);
