			ColorJugadorDefault(playerid);
			TrenTres = playerid;
		}
		if(!strcmp(npcname,"Tren_Cuatro"))
		{
			PutPlayerInVehicle(playerid, NPC_Tren[3], 0);
			SetPlayerSkin(playerid,71);
			ColorJugadorDefault(playerid);
			TrenCuatro = playerid;
		}
		if(!strcmp(npcname, "Alexis", true))
		{
			PutPlayerInVehicle(playerid, NPC_Tren[5], 0);
			SetPlayerSkin(playerid, 71);
			PlayerInfo[playerid][pDuty] = 1;
			ColorJugadorDefault(playerid);
		}
		if(!strcmp(npcname, "Marcelo", true))
		{
			SetPlayerSkin(playerid,281);
			Attach3DTextLabelToPlayer(Marcelo, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
		}
		if(!strcmp(npcname, "Alejandro", true))
		{
			SetPlayerSkin(playerid,71);
			Attach3DTextLabelToPlayer(Alejandro, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
		}
		if(!strcmp(npcname, "Camello", true))
		{
			SetPlayerSkin(playerid,29);
			Attach3DTextLabelToPlayer(CamelloTexto, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
			Camello = playerid;
		}
		if(!strcmp(npcname, "HippePepe", true))
		{
			SetPlayerSkin(playerid,50);
			Attach3DTextLabelToPlayer(HippePepe, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
		}
		if(!strcmp(npcname, "Hugo", true))
		{
			SetPlayerSkin(playerid,71);
			Attach3DTextLabelToPlayer(Hugo, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
		}
		if(!strcmp(npcname, "Mao", true))
		{
			SetPlayerSkin(playerid,57);
			Attach3DTextLabelToPlayer(Mao, playerid, 0.0, 0.0, 0.2);
			SetPlayerColor(playerid,0xFFFFFF00);
		}
		return 1;
	}
	if(PlayerInfo[playerid][jAdminNvl] <= 1)
	{
		SetPlayerSkillLevel(playerid, WEAPONSKILL_PISTOL, 0);
	}
	if(Specteando[playerid] == 1)
	{
		DarRopa(playerid);
		SetPlayerPos(playerid, SpectX[playerid],SpectY[playerid],SpectZ[playerid]);
		DarArmas(playerid);
		Specteando[playerid] = 0;
		SetCameraBehindPlayer(playerid);
	}
	if(PlayerInfo[playerid][jAdminNvl] > 5)
	{
		PlayerInfo[playerid][jAdminNvl] = 0;
	}
	if(PrimerSpawn[playerid] == 0)
	{
		new cadena[128];
		PlayerPlaySound(playerid, 3402, 0.0, 0.0, 0.0);
		LimpiarChat(playerid, 100, -1);
		if(PlayerInfo[playerid][jDia] != 0)
		{
			SCM(playerid, -1, "-----------------------------------------------------------------------------------------------------");
			format(cadena, sizeof(cadena), "Hola {00CCFF}%s{FFFFFF}, te vimos por última vez el %02d-%02d-%02d a las %02d:%02d:%02d", NombreJugador(playerid), PlayerInfo[playerid][jAno], PlayerInfo[playerid][jMes], PlayerInfo[playerid][jDia], PlayerInfo[playerid][jHora], PlayerInfo[playerid][jMinuto], PlayerInfo[playerid][jSegundo]);
			SCM(playerid, -1, cadena);
			SCM(playerid, -1, "-----------------------------------------------------------------------------------------------------");
		}
		else
		{
			format(cadena, sizeof(cadena), "{FFFFFF}Felicitaciones {00CCFF}%s{FFFFFF}, tu cuenta fue creada correctamente.", NombreJugador(playerid));
			SCM(playerid, -1, cadena);
		}
		if(PlayerInfo[playerid][pStats] == 0) SCM(playerid,-1,"Aun no has recibido tus stats, usa {00CC00}/recibir stats{FFFFFF} para recibirlas.");
		if(TiempoDuda[playerid] == 0)
		{
			TiempoDuda[playerid] = 240;
		}
		if(TiempoAD[playerid] == 0)
		{
			TiempoAD[playerid] = 240;
		}
		if(TiempoPDS[playerid] == 0) { TiempoPDS[playerid] = GetTickCount(); }
		if(PlayerInfo[playerid][vModelo] > 0 || PlayerInfo[playerid][vModelo2] > 0)
		{

			SetTimerEx("Autos",1000,false,"%d",playerid);
		}
		if(PlayerInfo[playerid][pInterior] > 0)
		{
			CongelarEx(playerid);
		}
		if(PlayerInfo[playerid][pMundoVirtual] > 0)
		{
			CongelarEx(playerid);
		}
		if(PlayerInfo[playerid][vip] == 1)
		{
			SetTimerEx("CheckVIP",1000,false,"d",playerid);
		}
		KillTimer(LoginTimer[playerid]);
		CalcularNivel(playerid);
		new year, month,day;
		new hour,minuite,second;
		getdate(year, month, day);
		gettime(hour,minuite,second);
		SCM(playerid, 0xFFFFFFFF, "Cargando datos del juego...");
		SCM(playerid, 0xDBED15FF, "El servidor está en constante crecimiento. Publica tus sugerencias en el foro.");
		SCM(playerid, 0xFFFFFFFF, "Escribe {DBED15}/ayuda{FFFFFF} para recibir ayuda.");
		SCM(playerid, 0xFFFFFFFF, "Para recibir ayuda de otros jugadores, usa {DBED15}/n pregunta{FFFFFF}.");
		new string2[128], Query[256], pais[60], plrIP[16];
		GetPlayerIp(playerid, plrIP, sizeof(plrIP));
		GetPlayerCountryNameEx(playerid, pais);
		format(string2, sizeof(string2), "%d/%02d/%02d %02d:%02d:%02d", day,month,year,hour,minuite,second);
		format(Query, sizeof(Query),"INSERT INTO `log_ingresos` (`Nombre`, `Pais`, `IP`, `Fecha`) VALUES ('%s','%s', '%s', '%s')", NombreJugador(playerid), pais, plrIP, string2);
		mysql_function_query(Conecction, Query, true, "OnQueryFinish", "ii", 0, playerid);
		MostrarMapas(playerid);
		LoadAnimLibs(playerid);
		TextDrawShowForPlayer(playerid, CN5);
		StopAudioStreamForPlayer(playerid);
		ClearAnimations(playerid);
		gettime(Hora, Minuto);
		SetPlayerTime(playerid,Hora,Minuto);
		SetPlayerInterior(playerid, PlayerInfo[playerid][pInterior]);
		SetPlayerVirtualWorld(playerid, PlayerInfo[playerid][pMundoVirtual]);
		SetProgressBarMaxValue(XP[playerid], PlayerInfo[playerid][NivelExp]);
		SetProgressBarValue(XP[playerid],PlayerInfo[playerid][jExperiencia]);
		UpdateProgressBar(XP[playerid],playerid);
		ShowProgressBarForPlayer(playerid, XP[playerid]);
		SetProgressBarValue(Barrased[playerid], PlayerInfo[playerid][pSed]);
		UpdateProgressBar(Barrased[playerid], playerid);
		SetProgressBarMaxValue(Barrased[playerid], 100);
		ShowProgressBarForPlayer(playerid, Barrased[playerid]);
		TextDrawShowForPlayer(playerid, Barra);
		TextDrawShowForPlayer(playerid, Titulo);
		ResetPlayerWeapons(playerid);
		DarArmas(playerid);
		TextDrawHideForPlayer(playerid, CN);
		TextDrawHideForPlayer(playerid, CN2);
		TextDrawHideForPlayer(playerid, CN3);
		TextDrawHideForPlayer(playerid, CN4);
		ColorJugadorDefault(playerid);
		ResetPlayerMoney(playerid);
		SetTimerEx("Logeado",1000,false,"d",playerid);
		SetTimerEx("Logeado2",3000,false,"d",playerid);
		Logeando[playerid] = 0;
		JugadorLogeado[playerid] = 1;
		DarRopa(playerid);
		LoadPrendas(playerid);
		SetPlayerStats(playerid);
	}
	if(VariableCaido[playerid] == 1)
	{
		PlayerInfo[playerid][Agonizando] = 0;
		ResetPlayerWeapons(playerid);
		DarArmas(playerid);
		DarRopa(playerid);
		TogglePlayerControllable(playerid,0);
		SetPVarInt(playerid, "IsFrozen", 0);
		SetPlayerHealth(playerid, 10.0);
		SCM(playerid, -1, "{DBED15}Los médicos no van a tratar con un delincuente.");
		SCM(playerid, -1, "{DBED15}Espera a que la policía te traslade.");
		ClearAnimations(playerid);
		ApplyAnimation(playerid, "SWEET", "Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0, 1);
		ApplyAnimation(playerid, "SWEET", "Sweet_injuredloop", 4.0, 1, 0, 0, 0, 0, 1);
		for(new i = 0; i < GetMaxPlayers(); i++)
		if(PlayerInfo[i][pDuty] == 1)
		{
			SetPlayerMarkerForPlayer(i, playerid, COLOR_GREEN2);
		}
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoCarcelS] > 0)
	{
		EnviarCarcel(playerid);
		Jail[playerid] = 1;
		return 1;
	}
	if(PlayerInfo[playerid][pTiempoJail] > 0)
	{
		SetTimerEx("EnviarJail", 2000, false, "d", playerid);
		SetTimerEx("TiempoJail",PlayerInfo[playerid][pTiempoJail],false,"d",playerid);
		San[playerid] = 1;
		return 1;
	}
	if(PlayerInfo[playerid][Agonizando] == 1 && GetPlayerInterior(playerid) == 0)
	{
		DarRopa(playerid);
		ResetPlayerWeapons(playerid);
		DarArmas(playerid);
		Muriendo[playerid] = 1;
		SetPlayerHealth(playerid, 100.0);
		DarDinero(playerid,0);
		ClearAnimations(playerid);
		ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0, 1);
		ApplyAnimation(playerid, "CRACK", "crckdeth2", 4.0, 1, 0, 0, 0, 0, 1);
		SendClientMessage(playerid, -1, "Estás muy malherido y comienzas a perder sangre.");
		SendClientMessage(playerid, -1, "Para llamar a una ambulancia, escribe {DBED15}/servicio medico");
		SendClientMessage(playerid, -1, "Para aceptar tu muerte, escribe {DBED15}/aceptar muerte");
	}
	else if(GetPlayerInterior(playerid) > 0 && GetPVarInt(playerid, "iSP1") == 0)
	{
		SetPlayerInterior(playerid,0);
		SetPlayerVirtualWorld(playerid, 0);
		if(PlayerInfo[playerid][pDuty] != 1) { QuitarArmas(playerid); }
		ApareceHospital(playerid);
	}
	else if(GetPVarInt(playerid, "iSP1") > 0)
	DeletePVar(playerid, "iSP1");
	if(PlayerInfo[playerid][Agonizando] == 2)
	{
		if(PlayerInfo[playerid][pDuty] != 1)
		{
			QuitarArmas(playerid);
		}
		ApareceHospital(playerid);
	}
	return 1;
}


