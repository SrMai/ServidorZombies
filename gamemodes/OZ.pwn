CMD:usar(playerid,params[])
{
	new item[32];
	if(!sscanf(params,"s[32]",item))
	{
		if(strcmp(item, "bidon", true)==0)
		{
			if(Bidon[playerid] == 0) return SCM(playerid, -1, "Tu no tienes un bidon de gasolina");
			if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Debes estar en un vehículo para utilizar el bidon");
			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SCM(playerid,-1,"Debes estar manejando el vehiculo para usar el bidon");
			{

				new autoid = GetPlayerVehicleID(playerid);
				new resta = 100-VehicleFuel[autoid];
				if(resta > 5)
				{
					VehicleFuel[autoid] += 5;
					Bidon[playerid] = 0;
					SCM(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros.");
					RemovePlayerAttachedObject(playerid, 2);
				}
				else if(resta < 5)
				{
					VehicleFuel[autoid] += resta;
					Bidon[playerid] = 0;
					SCM(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros.");
					RemovePlayerAttachedObject(playerid, 2);
				}
			}
		}
		if(strcmp(item, "prendas", true)==0)
		{
			if(PlayerInfo[playerid][pObj] == 0 && PlayerInfo[playerid][pObj2] == 0) return SCM(playerid,-1,"Tu no tienes ninguna prenda");
			new string[100],string2[200],Texto[100],Texto2[100],Texto3[100],Texto4[100];
			if(PlayerInfo[playerid][pObj] == 0) { Texto = "Vacío"; }
			else
			{
				if(UsandoObj[playerid] == 1) { format(string,100,"{00CC00}%s",GetWeaponNameEx(PlayerInfo[playerid][pObj])); }
				else { format(string,100,"%s",GetWeaponNameEx(PlayerInfo[playerid][pObj])); }
				Texto = string;
			}
			if(PlayerInfo[playerid][pObj2] == 0) { Texto2 = "Vacío"; }
			else
			{
				if(UsandoObj2[playerid] == 1) { format(string,100,"{00CC00}%s",GetWeaponNameEx(PlayerInfo[playerid][pObj2])); }
				else { format(string,100,"%s",GetWeaponNameEx(PlayerInfo[playerid][pObj2])); }
				Texto2 = string;
			}
			if(PlayerInfo[playerid][pObj3] == 0) { Texto3 = "Vacío"; }
			else
			{
				if(UsandoObj3[playerid] == 1) { format(string,100,"{00CC00}%s",GetWeaponNameEx(PlayerInfo[playerid][pObj3])); }
				else { format(string,100,"%s",GetWeaponNameEx(PlayerInfo[playerid][pObj3])); }
				Texto3 = string;
			}
			if(PlayerInfo[playerid][pObj4] == 0) { Texto4 = "Vacío"; }
			else
			{
				if(UsandoObj4[playerid] == 1) { format(string,100,"{00CC00}%s",GetWeaponNameEx(PlayerInfo[playerid][pObj4])); }
				else { format(string,100,"%s",GetWeaponNameEx(PlayerInfo[playerid][pObj4])); }
				Texto4 = string;
			}
			format(string2,100,"%s\n%s\n%s\n%s",Texto,Texto2,Texto3,Texto4);
			ShowPlayerDialog(playerid,DPRENDAS,DIALOG_STYLE_LIST,"Tus prendas",string2,"Elejir","Cerrar");
		}
	}
	return 1;
}

CMD:comprar(playerid, params[])
{
	new item[32],day,month,year,Query[200];
	getdate(day,month,year);
	if(IsPlayerConnected(playerid))
	if(!sscanf(params, "s[32]", item))
	{
		if(strcmp(item,"nivel",true)==0)
		{
			if(PlayerInfo[playerid][jNivel] >= 1)
			{
				new infostring[128];
				if(PlayerInfo[playerid][jExperiencia] < PlayerInfo[playerid][NivelExp])
				{
					format(infostring, 128, "Necesitas {F4FA58}%d{FFFFFF} puntos de respeto para subir de nivel, actualmente tienes %d",PlayerInfo[playerid][NivelExp],PlayerInfo[playerid][jExperiencia]);
					SendClientMessage(playerid, COLOR_BLANCO, infostring);
				}
				else
				{
					if(PlayerInfo[playerid][jDinero] < PlayerInfo[playerid][PrecioNivel])
					{
						format(infostring,128,"Necesitas {00CC00}$%d{FFFFFF} para comprar este nivel",PlayerInfo[playerid][PrecioNivel]);
						return SCM(playerid,-1,infostring);
					}
					SCM(playerid, 0xFFFF00FF,"Felicidades, aumentaste un nivel. Ahora tu pago diario será mayor.");
					SetPlayerStats(playerid);
					PlayerInfo[playerid][jNivel]++;
					SetPlayerScore(playerid, PlayerInfo[playerid][jNivel]);
					PlayerInfo[playerid][jExperiencia] = 0;
					QuitarDinero(playerid, PlayerInfo[playerid][PrecioNivel]);
					CalcularNivel(playerid);

					SetProgressBarMaxValue(XP[playerid], PlayerInfo[playerid][NivelExp]);
					SetProgressBarValue(XP[playerid], PlayerInfo[playerid][jExperiencia]);
					UpdateProgressBar(XP[playerid],playerid);
					Actualizar(playerid);
				}
			}
			return 1;
		}
		if(strcmp(item, "herramientas",true)==0)
		{
			if(IsPlayerInRangeOfPoint(playerid,5.0,-1072.1169,354.2132,1180.5259))
			{
				new string[160];
				format(string, 160, "{FFFFFF}Martillo - {00CC00}$20{FFFFFF}\n{FFFFFF}Destornillador - {00CC00}$25{FFFFFF}\n{FFFFFF}Barreta - {00CC00}$60{FFFFFF}\n{FFFFFF}Balde - {00CC00}$120");
				ShowPlayerDialog(playerid,HERRAMIENTAS, DIALOG_STYLE_LIST,"Herramientas",string,"Comprar","Cancelar");
			}
		}
		if(strcmp(item, "boleto",true)==0)
		{
			if(!IsAtTrenStation(playerid)) return SCM(playerid, -1, "No puedes comprar boletos en este lugar.");
			if(PlayerInfo[playerid][jDinero] < 30) return SCM(playerid, -1, "No tienes el dinero para comprar un boleto (30$).");
			SCM(playerid, -1, "Gracias por comprar un boleto de tren, solo es valido para un viaje.");
			QuitarDinero(playerid,30);
			Boletos[playerid] = 1;
		}
		if(strcmp(item, "sprunk",true)==0)
		{

			if(IsPlayerInRangeOfPoint(playerid,5.0,498.6868,-76.0385,998.7578) || IsPlayerInRangeOfPoint(playerid,5.0,-225.1540,1404.1327,27.7734))
			{
				if(PlayerInfo[playerid][jDinero] < 70) return SendClientMessage(playerid,-1,"No tienes el dinero suficiente ($70)");
				TieneSprunk[playerid] = 100;
				QuitarDinero(playerid, 70);
				SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DRINK_SPRUNK);
				SCM(playerid, -1, "Compraste una sprunk por {00F000}70$");
			}
		}
		if(strcmp(item, "vino",true)==0)
		{

			if(IsPlayerInRangeOfPoint(playerid,5.0,498.6868,-76.0385,998.7578) || IsPlayerInRangeOfPoint(playerid,5.0,-225.1540,1404.1327,27.7734))
			{

				if(PlayerInfo[playerid][jDinero] < 60) return SendClientMessage(playerid,-1,"No tienes el dinero suficiente ($60)");
				QuitarDinero(playerid, 60);
				SetPlayerSpecialAction(playerid,SPECIAL_ACTION_DRINK_WINE);
				SCM(playerid, -1, "Compraste un vino por {00F000}60$");
			}
		}
		if(strcmp(item, "cerveza",true)==0)
		{

			if(IsPlayerInRangeOfPoint(playerid,5.0,498.6868,-76.0385,998.7578) || IsPlayerInRangeOfPoint(playerid,5.0,-225.1540,1404.1327,27.7734))
