			}
		}
	}
	if(GetPlayerSpecialAction(playerid) == SPECIAL_ACTION_DRINK_SPRUNK && (newkeys & KEY_FIRE))
	{


		if(TieneSprunk[playerid] != 0)
		{

			PlayerInfo[playerid][pSed] -= 1;
			SetProgressBarValue(Barrased[playerid], PlayerInfo[playerid][pSed]);
			if(SedOculta[playerid] == 0)
			{
				UpdateProgressBar(Barrased[playerid], playerid);
			}
			TieneSprunk[playerid] -= 1;
			if(PlayerInfo[playerid][pSed] == 0)
			{

				ClearAnimations(playerid);
				TieneSprunk[playerid] = 0;
				PlayerInfo[playerid][pSed] = 0;
				SetPlayerSpecialAction(playerid,SPECIAL_ACTION_NONE);
			}
		}
	}
	return 1;
}

