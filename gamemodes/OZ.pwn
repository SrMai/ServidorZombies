CMD:llamar(playerid, params[])
{
	new string[128], phonenumb,pName[50];
	GetPlayerName(playerid, pName, 50);
	if(sscanf(params, "d", phonenumb)) return SendClientMessage(playerid, COLOR_WHITE, "USO: /llamar [Número]");
	if(PlayerInfo[playerid][pTiempoCarcelM] > 0) return SendClientMessage(playerid,COLOR_GREY,"[ERROR]: No puedes usar este comando mientras estás en prisión.");
	if(PlayerInfo[playerid][pNumero] == 0) return SendClientMessage(playerid, COLOR_GRAD2, "No tienes un teléfono.");
	if(PlayerInfo[playerid][pNumero] == phonenumb) return SCM(playerid,-1,"Número inválido");
	if(EnLlamada[playerid] == 1) return SCM(playerid,-1,"Ya estas en una llamada, usa /colgar para terminarla");
	format(string, sizeof(string), "%s saca su telèfono y marca un número.", pName);
	ProxDetector(30.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
	SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
	PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);
	for(new i=0;i<MAX_PLAYERS; i++)
	{
		if(phonenumb == 911)
		{
			PlayerInfo[playerid][MobileSub] = 911;
			SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
			SetTimerEx("LlamadaCops",2000,false,"d",playerid);
			SendClientMessage(playerid, -1, "{3399FF}[Central de Policía] {FFFFFF}Romina: Hola, por favor aguarde, ya te transfiero con un oficial de policía.");
			return 1;
		}
		if(PlayerInfo[i][pNumero] == phonenumb && phonenumb != 0)
		{
			if(EnLlamada[i] == 1) return SCM(playerid,-1,"El numero que usted marco se encuentra ocupado");
			if(TelefonoEstado[i] == 1) return SCM(playerid,-1,"El numero que marco esta fuera de linea");
			{
				new pName2[50];
				GetPlayerName(i,pName2,50);
				if(IsPlayerConnected(i)){
					if(i != INVALID_PLAYER_ID){
						if (PlayerInfo[i][MobileSub] == INVALID_PLAYER_ID)
						{


							SetPlayerSpecialAction(playerid, SPECIAL_ACTION_USECELLPHONE);
							format(string, sizeof(string), "El teléfono de %s está sonando.", pName2);
							ProxDetector(30.0, i, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
							format(string, sizeof(string), "Te están llamando por teléfono desde el número: {DBED15}%d{FFFFFF}, usa {DBED15}/atender{FFFFFF} o {DBED15}/colgar", PlayerInfo[playerid][pNumero]);
							SendClientMessage(i, -1, string);
							PlayerInfo[playerid][MobileSub] = i;
							PlayerInfo[i][MobileSub] = playerid;
							llamada(i);
							return 1;
						}
					}
				}
			}
		}
	}
	SendClientMessage(playerid, COLOR_GRAD2, "Su llamada no puede darse por el numero marcado, por favor, compruebe el número e inténtelo nuevamente.");
	SetTimerEx("StopCell",3000,false,"d",playerid);
	return 1;
}

CMD:sms(playerid, params[])
{
	new numero,mensaje[110],pName[32],string[128],pNameSub[50];
	if(!sscanf(params,"ds[110]",numero,mensaje))
	{

		if(PlayerInfo[playerid][pNumero] == 0) return SCM(playerid, -1, "Necesitas tener un teléfono para enviar un SMS");
		if(PlayerInfo[playerid][jDinero] < 10) return SCM(playerid, -1, "No tienes el dinero suficiente para enviar un sms ($15)");
		if(PlayerInfo[playerid][pNumero] != numero)
		{

			for(new i=0;i<MAX_PLAYERS;i++)
			{

				if(PlayerInfo[i][pNumero] == numero)
				{

					if(TelefonoEstado[i] == 0)
					{

						GetPlayerName(i,pName,32);
						GetPlayerName(playerid, pNameSub, 50);
						SCM(playerid, COLOR_DORADO,"Mensaje enviado con éxito.");
						format(string, 128,"((SMS de %s [%d])): %s",pNameSub,PlayerInfo[playerid][pNumero], mensaje);
						SCM(i,COLOR_DORADO,string);
						QuitarDinero(playerid, 15);
						return 1;
					}
					else return SCM(playerid,-1,"El número que has marcado se encuentra desconectado, intentalo más tarde.");
				}
			}
		}
	}
	else return SCM(playerid, -1, "Modo de uso /sms [número de teléfono] [mensaje]");
	return 1;
}

