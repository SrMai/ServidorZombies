public OnPlayerDisconnect(playerid, reason)
{
	new pName2[50],stringo[128];
	Specteando[playerid] = 0;
	KillTimer(TM[playerid]);
	KillTimer(VerAutos[playerid]);
	KillTimer(TCV[playerid]);
	KillTimer(TCV2[playerid]);
	KillTimer(AntiCarHacks[playerid]);
	KillTimer(BaulPB[playerid]);
	KillTimer(llamadasonido1[playerid]);
	KillTimer(llamadasonido2[playerid]);
	KillTimer(asd[playerid]);
	KillTimer(asdtren[playerid]);

	Emisoras[playerid][TransEmisora] = 0;
	Emisoras[playerid][CabinaOcupada] = -1;
	Emisoras[playerid][TransEmisora] = 1;
	Emisoras[playerid][CabinaOcupada] = 1;
	Emisoras[playerid][EmisoraID] = random(1000);
	CabinasOcupadasEmisoras[1][0] = 1;
	CabinasOcupadasEmisoras[1][1] = playerid;

	if(Emisoras[playerid][TransEmisora] == 1)
	{
		for(new emi = 1; emi < 4; emi++)
		{
			if(CabinasOcupadasEmisoras[emi][0] == 1)
			{
				if(Emisoras[playerid][CabinaOcupada] == emi)
				{
					if(Emisoras[playerid][TransEmisora] == 1)
					{
						StopAudioStreamForPlayer(playerid);
						CabinasOcupadasEmisoras[emi][0] = 0;
						CabinasOcupadasEmisoras[emi][1] = -1;
						CabinasOcupadasEmisoras[emi][2] = 0;
						Emisoras[playerid][TransEmisora] = 0;
						Emisoras[playerid][CabinaOcupada] = 0;
						new stronga[128];
						format(Emisoras[playerid][URLEmisora], 128, "http://fenixzone.sytes.net:8000/listen.pls");
						format(stronga, sizeof(stronga), "{FFFFFF}Radio disponible {C0C0C0}[N%d]", emi);
						UpdateDynamic3DTextLabelText(CabinasTextos[emi], COLOR_BLANCO, stronga);
						for(new id2 = 0; id2 < MAX_PLAYERS; id2++)
						{
							if(TieneMicrofono[id2] == emi)
							{
								TieneMicrofono[id2] = 0;
							}
							if(IsPlayerInDynamicArea(id2, AreaCabinas[emi]))
							{
								SendClientMessage(id2, COLOR_BLANCO, "La emisora que estabas escuchando dejo de transmitir.");
								StopAudioStreamForPlayer(id2);
							}
							if(IsPlayerInAnyVehicle(id2) && EmisoraEscuchando[GetPlayerVehicleID(id2)] == emi)
							{
								SendClientMessage(id2, COLOR_BLANCO, "La emisora que estabas escuchando dejo de transmitir.");
								StopAudioStreamForPlayer(id2);
							}
						}
						for(new vehicleid = 0; vehicleid < MAX_PLAYERS; vehicleid++)
						{
							if(EmisoraEscuchando[vehicleid] == emi)
							{
								EmisoraEscuchando[vehicleid] = -1;
							}
						}
					}
				}
			}
		}
	}

	if(PlayerInfo[playerid][pCama] > 0)
	{

		Camas[PlayerInfo[playerid][pCama]-1][PlayerInfo[playerid][pVwCama]] = 0;
	}
	if(IsValidObject(objeto1[playerid]))
	{

		DestroyObject(objeto1[playerid]);
	}
	if(IsValidObject(objeto2[playerid]))
	{

		DestroyObject(objeto2[playerid]);
	}
	if(IsValidObject(objeto3[playerid]))
	{

		DestroyObject(objeto3[playerid]);
	}
	if(EnCombate[playerid] == 1)
	{

		if(reason == 1)
		{

			if(NoQuitar[playerid] == 0 && PlayerInfo[playerid][pDuty] != 1)
			{

				GetPlayerName(playerid,pName2,50);
				QuitarArmas(playerid);
				format(stringo, 128, "%s(%d) perdió las armas por cerrar el juego en combate.",pName2,playerid);
				ProxDetector(15.0, playerid, stringo, COLOR_COMBATE,COLOR_COMBATE,COLOR_COMBATE,COLOR_COMBATE,COLOR_COMBATE);
				KillTimer(TimerCombate[playerid]);
			}
		}
	}
	new caller = PlayerInfo[playerid][MobileSub];
	GetPlayerName(caller, pName2, 50);
	if((IsPlayerConnected(caller) && caller != INVALID_PLAYER_ID)|| caller == 2540 || caller == 911)
	{
		if(caller < 50)
		SendClientMessage(caller,  -1, "Colgaron la llamada.");
		format(stringo, sizeof(stringo), "%s guarda su teléfono.", pName2);
		ProxDetector(30.0, caller, stringo, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		finllamada(caller);
		PlayerInfo[caller][MobileSub] = INVALID_PLAYER_ID;
		PlayerInfo[caller][Mobile] = INVALID_PLAYER_ID;
		PlayerInfo[playerid][MobileSub] = INVALID_PLAYER_ID;
		PlayerInfo[playerid][Mobile] = INVALID_PLAYER_ID;
		SetPlayerSpecialAction(caller, SPECIAL_ACTION_STOPUSECELLPHONE);
	}
	PlayerInfo[playerid][MobileSub] = INVALID_PLAYER_ID;
	PlayerInfo[playerid][Mobile] = INVALID_PLAYER_ID;
	if(Manejando[playerid] == 1)
	{

		DarArmas(playerid);
	}
	for(new i=0;i<MAX_PLAYERS;i++)
	{
		/*
		new string[128],pName[32];
		if(PlayerInfo[i][jAdminNvl] > 2)
		{

			switch(reason)
			{

				case 0:
				{

					GetPlayerName(playerid, pName, 32);
					format(string, 128, "%s ha salido del servidor, razón: Crash/timeout",pName);
					SCM(i, COLOR_GREY,string);
				}
				case 1:
				{

					GetPlayerName(playerid, pName, 32);
					format(string, 128, "%s ha salido del servidor, razón: /q",pName);
					SCM(i, COLOR_GREY,string);
				}
				case 2:
				{

					GetPlayerName(playerid, pName, 32);
					format(string, 128, "%s ha salido del servidor, razón: Kick/Ban",pName);
					SCM(i, COLOR_GREY,string);
				}
			}
		}
		*/
		if(PlayerInfo[i][pSpectating] == playerid)
		{
			PlayerInfo[i][pSpectating] = -1;
			TogglePlayerSpectating(i, false);
			SetCameraBehindPlayer(i);
			SetPlayerPos(i, PlayerInfo[i][jPos_x], PlayerInfo[i][jPos_y], PlayerInfo[i][jPos_z]);
			SetPlayerInterior(i, PlayerInfo[i][pInterior]);
			SetPlayerVirtualWorld(i, PlayerInfo[i][pMundoVirtual]);
			SendClientMessage(i, -1, "El jugador que estabas espectando se desconectó del servidor.");
		}
	}
	for(new i=0;i<MAX_VEHICLES;i++)
	{

		if(Chofer[i] == playerid)
		{

			Chofer[i] = INVALID_PLAYER_ID;
			CamionCargado[i] = 0;
		}
	}
	HideProgressBarForPlayer(playerid, TiempoBaul[playerid]);
	HideProgressBarForPlayer(playerid, XP[playerid]);
	HideProgressBarForPlayer(playerid, Barrased[playerid]);
	TextDrawHideForPlayer(playerid, Barra);
	TextDrawHideForPlayer(playerid, Titulo);
	KillTimer(TimerCheats[playerid]);
	DisablePlayerCheckpoint(playerid);

	Luces[autos[playerid]] = 0;
	MotorAuto[autos[playerid]] = 0;

	Luces[autos2[playerid]] = 0;
	MotorAuto[autos2[playerid]] = 0;

	Luces[autos3[playerid]] = 0;
	MotorAuto[autos3[playerid]] = 0;

	Luces[autos4[playerid]] = 0;
	MotorAuto[autos4[playerid]] = 0;

	SaveAccount(playerid);
	for(new i=0;i<MAX_VEHICLES;i++)
	{
		if(CamionCargado[CamionID[playerid]] == 1)
		{
			CamionCargado[CamionID[playerid]] = 0;
			Chofer[CamionID[playerid]] = INVALID_PLAYER_ID;
		}
	}
	return 1;
}

