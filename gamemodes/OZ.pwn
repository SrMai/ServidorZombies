CMD:radio(playerid, params[])
{
	new item[138];
	if(PlayerInfo[playerid][pRadio] != 1) return SendClientMessage(playerid, -1,"Tu no tienes un radio comunicador, puedes comprar uno en un 24/7");
	if(sscanf(params, "s[138]", item)) return SendClientMessage(playerid, -1,"Utiliza /radio on/off");
	{
		if(strcmp(item,	"on",true) == 0)
		{
			if(EstadoRadio[playerid] == 1) return SCM(playerid,-1,"Tu radio ya esta encendida");
			new string[256];
			if(EstadoRadio[playerid] == 1) return SendClientMessage(playerid, -1,"Tu radio ya esta encendida");
			format(string, 256, "%s enciende su radio comunicador",NombreJugador(playerid));
			EstadoRadio[playerid] = 1;
			ProxDetector(15.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		}
		if(strcmp(item, "off", true) == 0)
		{
			if(EstadoRadio[playerid] == 0) return SCM(playerid,-1,"Tu radio ya esta apagada");
			new string[256];
			if(EstadoRadio[playerid] == 0) return SendClientMessage(playerid, -1,"Tu radio ya esta apagada");
			format(string, 256, "%s apagó su radio comunicador",NombreJugador(playerid));
			EstadoRadio[playerid] = 0;
			ProxDetector(15.0, playerid, string, COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO,COLOR_VERDECLARO);
		}
	}
	return 1;
}

CMD:n(playerid, params[])
