		if(!strcmp(npcname,"Tren_Uno"))
		{
			PutPlayerInVehicle(playerid, NPC_Tren[0], 0);
			SetPlayerSkin(playerid,71);
			ColorJugadorDefault(playerid);
			TrenUno = playerid;
		}
		if(!strcmp(npcname,"Tren_Dos"))
		{
			PutPlayerInVehicle(playerid, NPC_Tren[4], 0);
			SetPlayerSkin(playerid,71);
			ColorJugadorDefault(playerid);
			TrenCinco = playerid;
		}
