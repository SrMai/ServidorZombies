	{
		if(!sscanf(params, "s[32]", item))
		{
			if(strcmp(item, "radio", true) == 0)
			{
			    if(PlayerToPoint(5.0, playerid, 393.7036, 185.4025, 1040.1156))
				{
					if(Emisoras[playerid][TieneEmisora] == 1) return SendClientMessage(playerid, COLOR_BLANCO, "Ya tienes una emisora.");
					if(PlayerInfo[playerid][vip] != 1) return SendClientMessage(playerid, COLOR_BLANCO, "Necesitas ser VIP2 para crear una estación de radio");
					if(PlayerInfo[playerid][jDinero] >= 15000)
					{
						ShowPlayerDialog(playerid, COMPRARRADIO, DIALOG_STYLE_INPUT,"Crear estación de radio","Ingresa el nombre de tu nueva estación de radio:","Crear","Cancelar");
						return 1;
					}
					else return SendClientMessage(playerid, COLOR_BLANCO, "Necesitas {00CC00}$15000{FFFFFF} para crear una estación de radio");
				}
				else return SendClientMessage(playerid, COLOR_BLANCO, "No estas donde se compras las radios.");
			}
