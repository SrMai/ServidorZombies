CMD:cargar(playerid,params[])
{
	new item[32];
	if(!sscanf(params,"s[32]", item))
	{


		if(strcmp(item, "camion", true) == 0)
		{


			new autoid;
			if(IsPlayerInAnyVehicle(playerid))
			autoid = GetPlayerVehicleID(playerid);
			if(PlayerInfo[playerid][jJob] != 3 && PlayerInfo[playerid][vipjob] != 3) return SCM(playerid,-1,"No eres camionero");
			{

				if(!CAMION(autoid) && !CAMIONVIP(autoid)) return SendClientMessage(playerid, -1, "Necesitas estar en un camión.");
				{

					for(new i=0;i<MAX_VEHICLES;i++)
					{

						if(Chofer[i] == playerid && Chofer[i] != INVALID_PLAYER_ID) return SCM(playerid,-1,"Ya tienes un camion cargado");
					}
					if(CamionCargado[autoid] == 0)
					{

						if(cavion[playerid] == 1) return SCM(playerid,-1,"Ha estas cargando tu avión");
						if(CargandoC[playerid] == 0)
						{

							SetPVarInt(playerid, "Camionero", 1);
							SendClientMessage(playerid, -1, "Ve a la posición marcada en el mapa para cargar tu camión");
							SetPlayerCheckpoint(playerid,-1711.7150,62.1237,3.5495,7.0);
							PuntoCX[playerid] = -1711.7150; PuntoCY[playerid] = 62.1237; PuntoCZ[playerid] = 3.5495;
							CamionID[playerid] = autoid;
						}
						else return SCM(playerid,-1,"Ya estas cargando tu camion");
					}
					else return SCM(playerid,-1,"Este camión ya esta cargado");
				}
			}
		}
		if(strcmp(item, "tanque", true)==0)
		{

			if(Bidon[playerid] == 0) return SCM(playerid, -1, "Tu no tienes un bidon de gasolina");
			if(!IsPlayerInAnyVehicle(playerid)) return SCM(playerid,-1,"Debes estar en un vehículo para utilizar el bidon");
			if(GetPlayerState(playerid) != PLAYER_STATE_DRIVER) return SCM(playerid,-1,"Debes estar manejando el vehiculo para usar el bidon");
			{
				new autoid = GetPlayerVehicleID(playerid);
				new resta = 100-VehicleFuel[autoid];
				if(resta > 5)
				{
					VehicleFuel[autoid] += 5;
					Bidon[playerid] = 0;
					SCM(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros.");
					RemovePlayerAttachedObject(playerid, 2);
				}
				else if(resta < 5)
				{
					VehicleFuel[autoid] += resta;
					Bidon[playerid] = 0;
					SCM(playerid, -1,"{DBED15}El tanque de combustible fue cargado con el bidón de 5 litros."); // No hay nada
					RemovePlayerAttachedObject(playerid, 2);
				}
			}

		}
	}
	return 1;
}

CMD:vender(playerid,params[])
{
	new item[32],ID,precio;
	if(TiempoVenta[playerid] == 0)
	{
		if(!sscanf(params, "s[32]dd", item,ID,precio))
		{
			if(precio < 0)
			return 1;
			if(PlayerInfo[playerid][jNivel] < 2)
			return SCM(playerid, -1, "Necesitas ser nivel 2 para hacer una venta");
			if(ID == playerid)
			return 1;
			if(!IsPlayerConnected(ID))
			return SendClientMessage(playerid, -1, "El jugador no está conectado");
			if(PlayerInfo[playerid][pTiempoCarcelS] > 0 || PlayerInfo[playerid][pTiempoJailS] > 0 || VariableCaido[playerid] == 1)
			return SCM(playerid,-1,"No puedes utilizar este comando en este momento");
			new pName[32],pNameid[32],string[256];
