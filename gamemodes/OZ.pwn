			else return SCM(playerid,-1,"Debes estar conduciendo un vehículo para abrir su capo");
		}
		if(strcmp(item, "puerta",true)==0)
		{
			for(new i=0;i<MAX_CASAS;i++)
			{
				if(CasasInfo[i][hID2] == PlayerInfo[playerid][EnCasa])
				{
					new ID = i;
					if(strcmp(CasasInfo[ID][hpropietario], pName2, true ) == 0)
					{
						if(CasasInfo[ID][hseguro] == 1) return SendClientMessage(playerid,-1,"La puerta de esta propiedad ya se encuentra cerrada.");
						CasasInfo[ID][hseguro] = 1;
						format(query, 200, "UPDATE `propiedades` SET `seguro`='1' WHERE `ID`='%d'",CasasInfo[i][hID2]);
						mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
						SendClientMessage(playerid,-1,"Cerraste la puerta de tu propiedad.");
						if(CasasInfo[ID][tipo] == 2)
						{
							DestroyDynamicPickup(CasaInfo3[ID]);
						}

					}
				}
			}
		}
		if(strcmp(item, "ropero",true)==0)
		{
			for(new i=0;i<MAX_CASAS;i++)
			{
				if(CasasInfo[i][hID2] == PlayerInfo[playerid][EnCasa])
				{
					new ID = i;
					if(PlayerInfo[playerid][EnCasa] != PlayerInfo[playerid][CasaID] && PlayerInfo[playerid][EnCasa] != PlayerInfo[playerid][CasaID2]) return SendClientMessage(playerid,-1,"No estas en ninguna de tus propiedades");
					{
						if(CasasInfo[ID][SeguroA] == 1) return SendClientMessage(playerid,-1,"La puerta del ropero ya se encuentra cerrada");
						CasasInfo[ID][SeguroA] = 1;
						format(query, 200, "UPDATE `propiedades` SET `SeguroA`='1' WHERE `ID`='%d'",CasasInfo[i][hID2]);
						mysql_function_query(Conecction, query, true, "OnQueryFinish", "ii", 0, playerid);
